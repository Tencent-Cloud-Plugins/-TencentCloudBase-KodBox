{
  "version": "2.0",
  "envId": "{{env.ENV_ID}}",
  "framework": {
    "name": "kodbox",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-container",
        "inputs": {
          "serviceName": "kodexplorer",
          "servicePath": "/",
          "uploadType": "image",
          "imageInfo": {
            "imageUrl": "ccr.ccs.tencentyun.com/tcb-images/kodbox-tcb:1.22"
          },
          "containerPort": 80,
          "customLogs": "stdout,/dev/stdout,/dev/stderr",
          "envVariables": {
          },
          "initialDelaySeconds": 10,
          "volumeMounts": {
            "/var/www/html": "kodbox-cfs"
          }
        }
      }
    },
    "requirement": {
      "addons": [
        {
          "type": "CFS",
          "name": "kodbox-cfs"
        }
      ],
      "environment": {
        "KOD_USERNAME": {
          "description": "管理控制台账号",
          "required": false,
          "disabled": true,
          "default": "admin"
        },
        "KOD_PASSWORD": {
          "description": "初始管理控制台密码",
          "required": false,
          "disabled": true,
          "default": ""
        }
      }
    }
  }
}